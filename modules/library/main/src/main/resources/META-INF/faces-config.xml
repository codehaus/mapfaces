<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <!-- ELResolver for the TreeTable component -->
    <application>
        <el-resolver>org.mapfaces.share.elresolver.CustomELResolver</el-resolver>
    </application>
    
    <!-- PhaseListener to load resources from jar -->
    <lifecycle>
        <phase-listener>org.mapfaces.share.listener.ResourcePhaseListener</phase-listener>
        <phase-listener>org.mapfaces.share.listener.ResourceLoaderPhaseListener</phase-listener>
        <phase-listener>org.mapfaces.share.listener.AjaxListener</phase-listener>
    </lifecycle>        
    
    
    <!-- *************************************************************** -->
    <!-- ******************** COMPONENTS ******************************* -->
    <!-- *************************************************************** -->
    <component>
        <component-type>org.mapfaces.treelayout.VisibilityColumn</component-type>
        <component-class>org.mapfaces.component.layercontrol.UIVisibilityColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.OpacityColumn</component-type>
        <component-class>org.mapfaces.component.layercontrol.UIOpacityColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.ElevationColumn</component-type>
        <component-class>org.mapfaces.component.layercontrol.UIElevationColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.TimeColumn</component-type>
        <component-class>org.mapfaces.component.layercontrol.UITimeColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.Abstract</component-type>
        <component-class>org.mapfaces.component.UIAbstract</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.ButtonBar</component-type>
        <component-class>org.mapfaces.component.UIButtonBar</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.models.Context</component-type>
        <component-class>org.mapfaces.component.models.UIContext</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.CursorTrack</component-type>
        <component-class>org.mapfaces.component.UICursorTrack</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.MapPane.Layer</component-type>
        <component-class>org.mapfaces.component.UILayer</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.LayerControl</component-type>
        <component-class>org.mapfaces.component.UILayerControl</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.MapPane</component-type>
        <component-class>org.mapfaces.component.UIMapPane</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.MapSize</component-type>
        <component-class>org.mapfaces.component.UIMapSize</component-class>
    </component>
     <!-- TimeLine component -->
        <component>
            <component-type>org.mapfaces.component.TimeLine</component-type>
            <component-class>org.mapfaces.component.timeline.UITimeLine</component-class>
        </component>
        <component>
            <component-type>org.mapfaces.component.Timeline.BandInfo</component-type>
            <component-class>org.mapfaces.component.timeline.UIBandInfo</component-class>
        </component>
        <component>
            <component-type>org.mapfaces.component.Timeline.HotZoneBandInfo</component-type>
            <component-class>org.mapfaces.component.timeline.UIHotZoneBandInfo</component-class>
        </component>
        <component>
            <component-type>SliderInput</component-type>
            <component-class>org.mapfaces.component.timeline.UISliderInput</component-class>
        </component>
     <!-- Tabbedpane component -->
     <component>
        <component-type>org.mapfaces.tabbedpane.TabPanel</component-type>
        <component-class>org.mapfaces.component.tabbedpane.UITabPanel</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.tabbedpane.tabpanel.TabItem</component-type>
        <component-class>org.mapfaces.component.tabbedpane.UITabItem</component-class>
    </component> 
    <!-- TreeTable component -->
     <component>
        <component-type>org.mapfaces.TreeTable</component-type>
        <component-class>org.mapfaces.component.tree.UITreeTable</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treetable.TreePanel</component-type>
        <component-class>org.mapfaces.component.tree.UITreePanel</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treetable.treepanel.TreeColumn</component-type>
        <component-class>org.mapfaces.component.tree.UITreeColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treetable.treepanel.Column</component-type>
        <component-class>org.mapfaces.component.tree.UIColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treetable.treepanel.CheckColumn</component-type>
        <component-class>org.mapfaces.component.tree.UICheckColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treetable.treepanel.TreeLines</component-type>
        <component-class>org.mapfaces.component.tree.UITreeLines</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treetable.treepanel.TreeNodeInfo</component-type>
        <component-class>org.mapfaces.component.tree.UITreeNodeInfo</component-class>
    </component>
    
    <!-- TreeLayout component -->
    <component>
        <component-type>org.mapfaces.treelayout.TreeTable</component-type>
        <component-class>org.mapfaces.component.treelayout.UITreeTable</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.treetable.TreePanel</component-type>
        <component-class>org.mapfaces.component.treelayout.UITreePanel</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.TreeColumn</component-type>
        <component-class>org.mapfaces.component.treelayout.UITreeColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.Column</component-type>
        <component-class>org.mapfaces.component.treelayout.UIColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.CheckColumn</component-type>
        <component-class>org.mapfaces.component.treelayout.UICheckColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.SelectOneMenuColumn</component-type>
        <component-class>org.mapfaces.component.treelayout.UISelectOneMenuColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.ImgColumn</component-type>
        <component-class>org.mapfaces.component.treelayout.UIImgColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.TreeLines</component-type>
        <component-class>org.mapfaces.component.treelayout.UITreeLines</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.TreeNodeInfo</component-type>
        <component-class>org.mapfaces.component.treelayout.UITreeNodeInfo</component-class>
    </component>
    
    
    <!-- *************************************************************** -->
    <!-- ******************** RENDER-KIT ******************************* -->
    <!-- *************************************************************** -->
    
    <render-kit>
        <!-- Map Renderers -->
        <renderer>
            <description>TimeColumn : This tag is used for creating  check columns of a table. (only use by the layer control)</description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.html.layercontrol.TimeColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.layercontrol.TimeColumnRenderer</renderer-class>
        </renderer>
         <renderer>
            <description>VisibilityColumn : This tag is used for creating  check columns of a table. (only use by the layer control)</description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.html.layercontrol.VisibilityColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.layercontrol.VisibilityColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>OpacityColumn : This tag is used for creating  a lsit of opacity(only use by the layer control).</description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.html.layercontrol.OpacityColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.layercontrol.OpacityColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>ElevationColumn : This tag is used for creating  a list of elevation (only use by the layer control).</description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.html.layercontrol.ElevationColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.layercontrol.ElevationColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Renders an map widget mapfaces. </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.html.Abstract</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.AbstractRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Renders a button bar to navigate on the map. </description>
            <component-family>org.mapfaces.ButtonBar</component-family>
            <renderer-type>org.mapfaces.renderkit.html.ButtonBar</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.ButtonBarRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Cursor position on map. </description>
            <component-family>org.mapfaces.CursorTrack</component-family>
            <renderer-type>org.mapfaces.renderkit.html.CursorTrack</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.CursorTrackRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Load a Web Map Context </description>
            <component-family>org.mapfaces.model.Context</component-family>
            <renderer-type>org.mapfaces.renderkit.html.models.Context</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.models.ContextRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Renders a layer . </description>
            <component-family>org.mapfaces.MapPane.Layer</component-family>
            <renderer-type>org.mapfaces.renderkit.html.MapPane.Layer</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.LayerRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Renders an LayerControl </description>
            <component-family>org.mapfaces.LayerControl</component-family>
            <renderer-type>org.mapfaces.renderkit.html.LayerControl</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.LayerControlRenderer</renderer-class>
        </renderer>
        <renderer> 
            <description>Renders an MapPane. </description>
            <component-family>org.mapfaces.MapPane</component-family>
            <renderer-type>org.mapfaces.renderkit.html.MapPane</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.MapPaneRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Modify MapPane viewport size. </description>
            <component-family>org.mapfaces.MapSize</component-family>
            <renderer-type>org.mapfaces.renderkit.html.MapSize</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.MapSizeRenderer</renderer-class>
        </renderer>
        
       
        <!-- TimeLine Renderers -->
        <renderer>
            <description>Timeline is a DHTML-based AJAX JSF component for visualizing time-based events</description>
            <component-family>org.mapfaces.component.TimeLine</component-family>
            <renderer-type>org.mapfaces.renderkit.HTMLTimeLine</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.timeline.TimeLineRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>BandInfo a component band for visualizing time-based events</description>
            <component-family>org.mapfaces.component.Timeline.BandInfo</component-family>
            <renderer-type>org.mapfaces.renderkit.Timeline.HTMLBandInfo</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.timeline.BandInfoRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>HotZoneBandInfo a component HotZone band for visualizing time-based events and multiples zones</description>
            <component-family>org.mapfaces.component.Timeline.HotZoneBandInfo</component-family>
            <renderer-type>org.mapfaces.renderkit.Timeline.HTMLHotZoneBandInfo</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.timeline.HotZoneBandInfoRenderer</renderer-class>
        </renderer>
        
         <!-- Tabbedpane Renderers -->
        <renderer>
            <description> TabPanel </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.HTMLTabPanel</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.tabbedpane.TabPanelRenderer</renderer-class>
        </renderer>
        <renderer>
            <description> TabItem </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.tabpanel.HTMLTabItem</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.tabbedpane.TabItemRenderer</renderer-class>
        </renderer>
        
        <!-- TreeTable Renderers -->
        <renderer>
            <description> Treetable : This tag is used to create tree table on the page. The component is rendered as an
            TreePanel component. UIColumn child components are responsible for rendering columns of the table. In these
            columns you can put any type of component like input text box, output text, command button etc. org:column>
            tag is used to create column. There can be many column tags within dataTable tag. You can set header and footer 
            in this table.
            </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.HTMLTreeTable</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.tree.TreeTableRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treepanel </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.treetable.HTMLTreePanel</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.tree.TreePanelRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treecolumn : This tag is used for creating columns of a table. This tag creates a data column inside 
            a tree data table
            </description>
            <component-family>org.mapfaces.treetable.treepanel.TreeColumn</component-family>
            <renderer-type>org.mapfaces.renderkit.treetable.treepanel.HTMLTreeColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.tree.TreeColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Column : This tag is used for creating columns of a table. This tag creates a data column inside 
            a tree data table
            </description>
            <component-family>org.mapfaces.treetable.treepanel.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.treetable.treepanel.HTMLColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.tree.ColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treelines </description>
            <component-family>org.mapfaces.treetable.treepanel.TreeLines</component-family>
            <renderer-type>org.mapfaces.renderkit.treetable.treepanel.HTMLTreeLines</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.tree.TreeLinesRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treenodeinfo </description>
             <component-family>org.mapfaces.treetable.treepanel.treelines.TreeNodeInfo</component-family>
            <renderer-type>org.mapfaces.renderkit.treetable.treepanel.HTMLTreeNodeInfo</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.tree.TreeNodeInfoRenderer</renderer-class>
        </renderer>
        
        <!-- treeLayout -->
        <renderer>
            <description> TreeLayoutTable </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLTreeTable</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.TreeTableRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treepanel </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLTreePanel</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.TreePanelRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treecolumn : This tag is used for creating columns of a table. This tag creates a data column inside 
            a tree data table
            </description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLTreeColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.TreeColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Column : This tag is used for creating columns of a table. This tag creates a data column inside 
            a tree data table
            </description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.ColumnRenderer</renderer-class>
        </renderer>
         <renderer>
            <description>CheckColumn : This tag is used for creating  check columns of a table.
            </description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLCheckColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.CheckColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>SelectOneMenuColumn : This tag is used for creating  check columns of a table.
            </description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLSelectOneMenuColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.SelectOneMenuColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>ImgColumn : This tag is used for creating  check columns of a table.
            </description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLImgColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.ImgColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treelines </description>
            <component-family>org.mapfaces.treelayout.TreeLines</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLTreeLines</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.TreeLinesRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treenodeinfo </description>
             <component-family>org.mapfaces.treelayout.TreeNodeInfo</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLTreeNodeInfo</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.TreeNodeInfoRenderer</renderer-class>
        </renderer>
    </render-kit>
</faces-config>

    <!-- CursorTrack -->
    <tag>
        <name>CursorTrack</name>
        <tag-class>org.mapfaces.taglib.CursorTrackTag</tag-class>
        <body-content>JSP</body-content>
        <description><![CDATA[This is the tag that creates a component widget to track the mouse cursor and display his position in pixel, xy, lon/lat, Degrees Minutes Sec or Degrees Minutes.]]></description>
        &commons_attributes;
        <attribute>
            <name>value</name>
            <deferred-value>
                <type>org.mapfaces.model.ViewContextType</type>
            </deferred-value>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
            <description><![CDATA[The new context defined by the user, the type is an Object ViewContextType.]]></description>
        </attribute>
        <attribute>
            <name>showPX</name>
            <deferred-value>
                <type>boolean</type>
            </deferred-value>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
            <description><![CDATA[Add pixel dipslaying.]]></description>
        </attribute>
        <attribute>
            <name>showXY</name>
            <deferred-value>
                <type>boolean</type>
            </deferred-value>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
            <description><![CDATA[Add xy dipslaying.]]></description>

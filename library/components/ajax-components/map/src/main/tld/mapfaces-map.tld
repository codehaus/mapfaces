    <!-- DataRequest -->
    <tag>
        <name>DataRequest</name>
        <tag-class>org.mapfaces.taglib.DataRequestTag</tag-class>
        <body-content>JSP</body-content>
        <description><![CDATA[This is the tag that creates a component to perform a request and set an output as a child component like popup or others output component.]]></description>
        <attribute>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
            <description><![CDATA[The component identifier for this component.  This value must be

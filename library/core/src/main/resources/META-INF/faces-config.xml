<?xml version="1.0" encoding="UTF-8"?>
<faces-config       version="1.2"
                                xmlns="http://java.sun.com/xml/ns/javaee"
                                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                                xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    
    <!-- PhaseListener to load resources from jar -->
    <lifecycle>
        <phase-listener>org.mapfaces.share.listener.ResourcePhaseListener</phase-listener>
        <phase-listener>org.mapfaces.share.listener.ResourceLoaderPhaseListener</phase-listener>
        <phase-listener>org.mapfaces.share.listener.DetectBrowserListener</phase-listener>
        <!--phase-listener>org.mapfaces.share.listener.PhaseTracker</phase-listener-->
    </lifecycle>
    
</faces-config>

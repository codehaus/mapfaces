<?xml version="1.0" encoding="UTF-8"?>
<OWSContext version="0.3.0" id="ows-context-ex-1-v3" xmlns="http://www.opengis.net/ows-context" xmlns:gml="http://www.opengis.net/gml" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:ogc="http://www.opengis.net/ogc" xmlns:ows="http://www.opengis.net/ows" xmlns:sld="http://www.opengis.net/sld" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/ows-context http://www.ogcnetwork.net/schemas/owc/0.3.0/owsContext.xsd">
	<General>                
		<Window width="1000" height="500"/>
		<ows:BoundingBox crs="EPSG:4326">
			<!--ows:LowerCorner>-117.44667178362664 32.57086210449395</ows:LowerCorner>
			<ows:UpperCorner>-116.74066794885977 32.921986352104064</ows:UpperCorner-->
                        
			<ows:LowerCorner>142 -45</ows:LowerCorner>
			<ows:UpperCorner>150 -38</ows:UpperCorner>
		</ows:BoundingBox>
		<ows:Title>OWS Context version 0.3.0 tasmania WMS example</ows:Title>
	</General>
	
	<ResourceList>
		
		<!-- WMS Example -->
               <Layer name="BlueMarble" queryable="1" hidden="0">                        
			<ows:Title>Blue Marble</ows:Title>
			<ows:OutputFormat>image/png</ows:OutputFormat>
			<Server service="urn:ogc:serviceType:WMS" version="1.1.1">
                                <OnlineResource xlink:type="simple" xlink:href="http://demo.geomatys.fr/constellation/WS/wms"/>
			</Server>                    
		</Layer>                
                <Layer name="tasmania_state_boundaries" queryable="1" hidden="0">
			<ows:Title>Tasmania boundaries</ows:Title>
			<ows:OutputFormat>image/png</ows:OutputFormat>
			<Server service="urn:ogc:serviceType:WMS" version="1.1.1">
				<OnlineResource xlink:href="http://demo.geomatys.fr/constellation/WS/wms"/>
			</Server>
                        <StyleList>
                            <Style current="1"> 
                                <Name>world</Name>
                                <Title>world style</Title>
                            </Style>
                        </StyleList>  
		</Layer>
		<Layer name="tasmania_water_bodies" queryable="1" hidden="0">
			<ows:Title>Tasmania_water_bodies</ows:Title>
			<ows:OutputFormat>image/png</ows:OutputFormat>
			<Server service="urn:ogc:serviceType:WMS" version="1.1.1">
				<OnlineResource xlink:href="http://demo.geomatys.fr/constellation/WS/wms"/>
			</Server>                             
                        <StyleList>
                            <Style current="1"> 
                                <Name></Name>
                                <Title></Title>
                            </Style>
                            <Style> 
                                <Name>world</Name>
                                <Title>world style</Title>
                                <LegendURL width="30" height="20">
                                    <Format>image/png</Format>
                                    <OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple" xlink:href="http://demo.geomatys.fr/constellation/WS/wms/GetLegendGraphic?VERSION=1.0.0&amp;FORMAT=image/png&amp;WIDTH=30&amp;HEIGHT=20&amp;LAYER=tasmania_water_bodies&amp;style=world"/>
                                </LegendURL>
                            </Style>
                            <Style> 
                                <SLD>
                                     <OnlineResource xlink:href="http://demo.geomatys.fr/geoserver/data/styles/green.sld"/> 
                                </SLD>
                                <LegendURL width="30" height="30">
                                    <Format>image/jpeg</Format>
                                    <OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple" xlink:href="http://demo.geomatys.fr/constellation/WS/wms/GetLegendGraphic?VERSION=1.0.0&amp;FORMAT=image/jpeg&amp;WIDTH=30&amp;HEIGHT=30&amp;LAYER=tasmania_water_bodies&amp;sld=http://demo.geomatys.fr/geoserver/data/styles/green.sld"/>
                                </LegendURL>
                            </Style>
                            <Style> 
                                <SLD>
                                    <StyledLayerDescriptor version="1.0.0" xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd" xmlns="http://www.opengis.net/sld"  xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                                      <NamedLayer>
                                        <Name>tasmania_water_bodies</Name>
                                        <UserStyle>
                                          <Name>water_bodies</Name>
                                          <FeatureTypeStyle>
                                            <Rule>
                                              <Name>R</Name>
                                              <PolygonSymbolizer>                                                  
                                               <Fill>
                                                  <CssParameter name="fill">#ff0000</CssParameter>
                                                  <CssParameter name="stroke-width">3</CssParameter>
                                                </Fill>
                                               <Stroke>
                                                  <CssParameter name="stroke">#1200ff</CssParameter>
                                                  <CssParameter name="stroke-width">1</CssParameter>
                                                </Stroke>
                                              </PolygonSymbolizer>
                                            </Rule>
                                            </FeatureTypeStyle>
                                        </UserStyle>
                                      </NamedLayer>
                                    </StyledLayerDescriptor>
                                </SLD>
                                <LegendURL width="40" height="40">
                                    <Format>image/gif</Format>
                                    <OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple" xlink:href="http://demo.geomatys.fr/constellation/WS/wms/GetLegendGraphic?VERSION=1.0.0&amp;FORMAT=image/gif&amp;WIDTH=40&amp;HEIGHT=40&amp;LAYER=tasmania_water_bodies"/>
                                </LegendURL>
                            </Style>
                        </StyleList>  
		</Layer>
              <Layer name="tasmania_roads" queryable="1" hidden="0">
			<ows:Title>Tasmania_roads</ows:Title>
			<ows:OutputFormat>image/png</ows:OutputFormat>
			<Server service="urn:ogc:serviceType:WMS" version="1.1.1">
				<OnlineResource xlink:href="http://demo.geomatys.fr/constellation/WS/wms"/>
			</Server>
                        <StyleList>
                            <Style> 
                                <Name></Name>
                                <Title></Title>
                            </Style>
                            <Style> 
                                <Name>green</Name>
                                <Title>Green style</Title>
                            </Style>
                            <Style> 
                                <SLD>
                                     <OnlineResource xlink:href="http://demo.geomatys.fr/geoserver/data/styles/green.sld"/> 
                                </SLD>
                            </Style>
                            <Style current="1"> 
                                <SLD>
                                    <StyledLayerDescriptor version="1.0.0" xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd" xmlns="http://www.opengis.net/sld"  xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                                      <NamedLayer>
                                        <Name>tasmania_roads</Name>
                                        <UserStyle>
                                          <FeatureTypeStyle>
                                            <Rule>
                                              <Name>R</Name>
                                              <LineSymbolizer>
                                                <Stroke>
                                                  <CssParameter name="stroke">#00abff</CssParameter>
                                                  <CssParameter name="stroke-width">3</CssParameter>
                                                </Stroke>
                                              </LineSymbolizer>
                                            </Rule>
                                            </FeatureTypeStyle>
                                        </UserStyle>
                                      </NamedLayer>
                                    </StyledLayerDescriptor>
                                </SLD>
                            </Style>
                        </StyleList>  
		</Layer>
                <Layer name="tasmania_cities" queryable="1" hidden="0">
			<ows:Title>Tasmania_cities</ows:Title>
			<ows:OutputFormat>image/png</ows:OutputFormat>
			<Server service="urn:ogc:serviceType:WMS" version="1.1.1">
				<OnlineResource xlink:href="http://demo.geomatys.fr/constellation/WS/wms"/>
			</Server>
                        <StyleList>
                            <Style> 
                                <Name></Name>
                                <Title></Title>
                            </Style>
                            <Style> 
                                <Name>capitals</Name>
                                <Title>capitals style</Title>
                            </Style>
                            <Style current="1"> 
                                <SLD>
                                     <OnlineResource xlink:href="http://demo.geomatys.fr/geoserver/data/styles/tasmania_cities.sld"/> 
                                </SLD>
                            </Style>
                             <Style> 
                                <SLD>
                                    <StyledLayerDescriptor version="1.0.0" xmlns="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                                        <NamedLayer>
                                            <Name>tasmania_cities</Name>
                                            <UserStyle>
                                                <Name>cities</Name>
                                                <FeatureTypeStyle>
                                                    <Rule>
                                                        <Name>R</Name>
                                                        <PointSymbolizer>
                                                            <Graphic>
                                                                <wellknownname>circle</wellknownname>
                                                                <Mark>
                                                                    <Fill>
                                                                        <CssParameter name="fill">#121112</CssParameter>
                                                                    </Fill>                                                                    
                                                                    <Stroke>
                                                                      <CssParameter name="stroke">#00abff</CssParameter>
                                                                      <CssParameter name="stroke-width">1</CssParameter>
                                                                    </Stroke>
                                                                </Mark>
                                                                <Size>11</Size>
                                                            </Graphic>
                                                        </PointSymbolizer>                                                        
                                                    </Rule>
                                                 </FeatureTypeStyle>
                                             </UserStyle>
                                        </NamedLayer>
                                    </StyledLayerDescriptor>
                                </SLD>                                
                            </Style>
                        </StyleList>  
		</Layer>
	</ResourceList>
</OWSContext>

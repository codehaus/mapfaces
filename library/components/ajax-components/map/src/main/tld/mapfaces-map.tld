        <attribute>
            <name>title</name>
            <deferred-value>
                <type>java.lang.String</type>
            </deferred-value>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
            <description>
                The title of the panel header.
            </description>
        </attribute>
        <attribute>
            <name>headerStyle</name>
            <deferred-value>
                <type>java.lang.String</type>
            </deferred-value>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
            <description>
                The style of the header div.
            </description>
        </attribute>
        <attribute>
            <name>headerStyleClass</name>
            <deferred-value>
                <type>java.lang.String</type>
            </deferred-value>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
            <description>
                The style class of the header div.
            </description>
        </attribute>
        <attribute>
            <name>bodyStyle</name>
            <deferred-value>
                <type>java.lang.String</type>
            </deferred-value>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
            <description>
                The style of the body content.
            </description>
        </attribute>
        <attribute>
            <name>bodyStyleClass</name>
            <deferred-value>
                <type>java.lang.String</type>
            </deferred-value>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
            <description>
                The style class of the body.
            </description>
        </attribute>
        <attribute>
            <name>height</name>
            <deferred-value>
                <type>java.lang.Integer</type>
            </deferred-value>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
            <description>
                this is the height of the panel.
            </description>
        </attribute>
        <attribute>
            <name>width</name>
            <deferred-value>
                <type>java.lang.Integer</type>
            </deferred-value>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
            <description>
                this is the width of the panel.
            </description>
        </attribute>
    </tag>

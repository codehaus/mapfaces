<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
   <!-- TreeLayout component -->
    
    <component>
        <component-type>org.mapfaces.treelayout.TreeTable</component-type>
        <component-class>org.mapfaces.component.treelayout.UITreeTable</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.treetable.TreePanel</component-type>
        <component-class>org.mapfaces.component.treelayout.UITreePanel</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.TreeColumn</component-type>
        <component-class>org.mapfaces.component.treelayout.UITreeColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.Column</component-type>
        <component-class>org.mapfaces.component.treelayout.UIColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.CheckColumn</component-type>
        <component-class>org.mapfaces.component.treelayout.UICheckColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.SelectOneMenuColumn</component-type>
        <component-class>org.mapfaces.component.treelayout.UISelectOneMenuColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.ImgColumn</component-type>
        <component-class>org.mapfaces.component.treelayout.UIImgColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.TreeLines</component-type>
        <component-class>org.mapfaces.component.treelayout.UITreeLines</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.TreeNodeInfo</component-type>
        <component-class>org.mapfaces.component.treelayout.UITreeNodeInfo</component-class>
    </component>
    <render-kit>        
        
        <renderer>
            <description> TreeLayoutTable </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLTreeTable</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.TreeTableRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treepanel </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLTreePanel</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.TreePanelRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treecolumn : This tag is used for creating columns of a table. This tag creates a data column inside 
            a tree data table
            </description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLTreeColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.TreeColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Column : This tag is used for creating columns of a table. This tag creates a data column inside 
            a tree data table
            </description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.ColumnRenderer</renderer-class>
        </renderer>
         <renderer>
            <description>CheckColumn : This tag is used for creating  check columns of a table.
            </description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLCheckColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.CheckColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>SelectOneMenuColumn : This tag is used for creating  check columns of a table.
            </description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLSelectOneMenuColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.SelectOneMenuColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>ImgColumn : This tag is used for creating  check columns of a table.
            </description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLImgColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.ImgColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treelines </description>
            <component-family>org.mapfaces.treelayout.TreeLines</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLTreeLines</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.TreeLinesRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Treenodeinfo </description>
             <component-family>org.mapfaces.treelayout.TreeNodeInfo</component-family>
            <renderer-type>org.mapfaces.renderkit.treelayout.HTMLTreeNodeInfo</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.treelayout.TreeNodeInfoRenderer</renderer-class>
        </renderer>
    </render-kit>
</faces-config>

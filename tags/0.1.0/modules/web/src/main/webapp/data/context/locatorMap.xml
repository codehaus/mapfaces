		<ows:Title>OWS Context version 0.3.0, Metacarta world map example</ows:Title>
	</General>
	<ResourceList>
		<!-- WMS Example -->
                 <Layer name="basic" queryable="0" hidden="0">                        
			<ows:Title>basic</ows:Title>
			<ows:OutputFormat>image/jpeg</ows:OutputFormat>
			<Server service="urn:ogc:serviceType:WMS" version="1.1.1">
                                <OnlineResource xlink:type="simple" xlink:href="http://labs.metacarta.com/wms/vmap0"/>
			</Server>                    
		</Layer>         
	</ResourceList>

<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <!-- ELResolver for the TreeTable component -->
    <application>
        <el-resolver>org.mapfaces.share.elresolver.CustomELResolver</el-resolver>
    </application>
    
    <!-- PhaseListener to load resources from jar -->
    <lifecycle>
        <phase-listener>org.mapfaces.share.listener.ResourcePhaseListener</phase-listener>
        <phase-listener>org.mapfaces.share.listener.ResourceLoaderPhaseListener</phase-listener>
        <phase-listener>org.mapfaces.share.listener.AjaxListener</phase-listener>
    </lifecycle>        
    
    <!-- Mapfaces components -->
    <component>
        <component-type>org.mapfaces.treelayout.VisibilityColumn</component-type>
        <component-class>org.mapfaces.component.layercontrol.UIVisibilityColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.OpacityColumn</component-type>
        <component-class>org.mapfaces.component.layercontrol.UIOpacityColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.ElevationColumn</component-type>
        <component-class>org.mapfaces.component.layercontrol.UIElevationColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.treelayout.TimeColumn</component-type>
        <component-class>org.mapfaces.component.layercontrol.UITimeColumn</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.Abstract</component-type>
        <component-class>org.mapfaces.component.UIAbstract</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.ButtonBar</component-type>
        <component-class>org.mapfaces.component.UIButtonBar</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.models.Context</component-type>
        <component-class>org.mapfaces.component.models.UIContext</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.CursorTrack</component-type>
        <component-class>org.mapfaces.component.UICursorTrack</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.MapPane.Layer</component-type>
        <component-class>org.mapfaces.component.UILayer</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.LayerControl</component-type>
        <component-class>org.mapfaces.component.UILayerControl</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.MapPane</component-type>
        <component-class>org.mapfaces.component.UIMapPane</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.component.MapSize</component-type>
        <component-class>org.mapfaces.component.UIMapSize</component-class>
    </component>    
     <!-- Tabbedpane component -->
     <component>
        <component-type>org.mapfaces.tabbedpane.TabPanel</component-type>
        <component-class>org.mapfaces.component.tabbedpane.UITabPanel</component-class>
    </component>
    <component>
        <component-type>org.mapfaces.tabbedpane.tabpanel.TabItem</component-type>
        <component-class>org.mapfaces.component.tabbedpane.UITabItem</component-class>
    </component>    
    <render-kit>
        <!-- Map Renderers -->
        <renderer>
            <description>TimeColumn : This tag is used for creating  check columns of a table. (only use by the layer control)</description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.html.layercontrol.TimeColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.layercontrol.TimeColumnRenderer</renderer-class>
        </renderer>
         <renderer>
            <description>VisibilityColumn : This tag is used for creating  check columns of a table. (only use by the layer control)</description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.html.layercontrol.VisibilityColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.layercontrol.VisibilityColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>OpacityColumn : This tag is used for creating  a lsit of opacity(only use by the layer control).</description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.html.layercontrol.OpacityColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.layercontrol.OpacityColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>ElevationColumn : This tag is used for creating  a list of elevation (only use by the layer control).</description>
            <component-family>org.mapfaces.treelayout.Column</component-family>
            <renderer-type>org.mapfaces.renderkit.html.layercontrol.ElevationColumn</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.layercontrol.ElevationColumnRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Renders an map widget mapfaces. </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.html.Abstract</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.AbstractRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Renders a button bar to navigate on the map. </description>
            <component-family>org.mapfaces.ButtonBar</component-family>
            <renderer-type>org.mapfaces.renderkit.html.ButtonBar</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.ButtonBarRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Cursor position on map. </description>
            <component-family>org.mapfaces.CursorTrack</component-family>
            <renderer-type>org.mapfaces.renderkit.html.CursorTrack</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.CursorTrackRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Load a Web Map Context </description>
            <component-family>org.mapfaces.model.Context</component-family>
            <renderer-type>org.mapfaces.renderkit.html.models.Context</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.models.ContextRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Renders a layer . </description>
            <component-family>org.mapfaces.MapPane.Layer</component-family>
            <renderer-type>org.mapfaces.renderkit.html.MapPane.Layer</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.LayerRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Renders an LayerControl </description>
            <component-family>org.mapfaces.LayerControl</component-family>
            <renderer-type>org.mapfaces.renderkit.html.LayerControl</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.LayerControlRenderer</renderer-class>
        </renderer>
        <renderer> 
            <description>Renders an MapPane. </description>
            <component-family>org.mapfaces.MapPane</component-family>
            <renderer-type>org.mapfaces.renderkit.html.MapPane</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.MapPaneRenderer</renderer-class>
        </renderer>
        <renderer>
            <description>Modify MapPane viewport size. </description>
            <component-family>org.mapfaces.MapSize</component-family>
            <renderer-type>org.mapfaces.renderkit.html.MapSize</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.MapSizeRenderer</renderer-class>
        </renderer>
        
         <!-- Tabbedpane Renderers -->
        <renderer>
            <description> TabPanel </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.HTMLTabPanel</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.tabbedpane.TabPanelRenderer</renderer-class>
        </renderer>
        <renderer>
            <description> TabItem </description>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>org.mapfaces.renderkit.tabpanel.HTMLTabItem</renderer-type>
            <renderer-class>org.mapfaces.renderkit.html.tabbedpane.TabItemRenderer</renderer-class>
        </renderer>
    </render-kit>
</faces-config>

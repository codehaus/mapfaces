<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee  http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
       
    
    <!-- Managed Beans -->
    <managed-bean>
        <managed-bean-name>switchTab</managed-bean-name>
        <managed-bean-class>org.mapfaces.template.bean.TabsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>init</managed-bean-name>
        <managed-bean-class>org.mapfaces.template.bean.InitBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
     <managed-bean>
        <managed-bean-name>metadata</managed-bean-name>
        <managed-bean-class>org.mapfaces.web.bean.MetadataBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
        
</faces-config>
